<table cellspacing="0" cellpadding="0" border="0" style="max-width:600px">
  <tbody>
    <tr>
      <td>
        <table width="100%" cellspacing="0" cellpadding="0" border="0" bgcolor="#E6E6E6" style="min-width:332px;max-width:600px;border:1px solid #e0e0e0;border-bottom:0;border-top-left-radius:3px;border-top-right-radius:3px">
          <tbody>
            <tr>
              <td height="18px" colspan="3"></td>
            </tr>
            <tr>
              <td width="32px"></td>
              <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:24px;color:#000000;line-height:1.25">
                Nuevo mensaje desde la página mayacom.com.uy
              </td>
              <td width="32px"></td>
            </tr>
            <tr>
              <td height="18px" colspan="3"></td>
            </tr>
          </tbody>
        </table>
      </td>
    </tr>
    <tr>
      <td>
        <table width="100%" cellspacing="0" cellpadding="0" border="0" bgcolor="#FAFAFA" style="min-width:332px;max-width:600px;border:1px solid #f0f0f0;border-bottom:1px solid #c0c0c0;border-top:0;border-bottom-left-radius:3px;border-bottom-right-radius:3px">
          <tbody>
            <tr height="16px"></tr>
            <tr>
              <td>
                <table cellspacing="0" cellpadding="0" border="0" style="min-width:300px">
                  <tbody>
                    <tr>
                      <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:13px;color:#202020;line-height:1.5;font-weight: bold;">
                        Nombre:
                      </td>
                    </tr>
                    <tr>
                      <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:13px;color:#202020;line-height:1.5">
                        <%= @name %>
                      </td>
                    </tr>
                    <tr>
                      <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:13px;color:#202020;line-height:1.5;font-weight: bold;">
                        Email:
                      </td>
                    </tr>
                    <tr>
                      <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:13px;color:#202020;line-height:1.5">
                        <%= @email %>
                      </td>
                    </tr>
                    <tr>
                      <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:13px;color:#202020;line-height:1.5;font-weight: bold;">
                        Teléfono:
                      </td>
                    </tr>
                    <tr>
                      <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:13px;color:#202020;line-height:1.5">
                        <%= @tel %>
                      </td>
                    </tr>
                    <tr>
                      <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:13px;color:#202020;line-height:1.5;font-weight: bold;">
                        Mensaje:
                      </td>
                    </tr>
                    <tr>
                      <td style="font-family:Roboto-Regular,Helvetica,Arial,sans-serif;font-size:13px;color:#202020;line-height:1.5">
                        <%= @message %>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </td>
            </tr>
            <tr height="16px"></tr>
          </tbody>
        </table>
      </td>
    </tr>
  </tbody>
</table>